{
  "name": "@sepveneto/report-core",
  "version": "0.7.6",
  "description": "",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "bootstrap": "wasm-pack build ../sign --target no-modules --out-dir ../sign/pkg-client --mode no-install --scope client",
    "build": "tsup",
    "release": "bumpp --tag core@",
    "build:fix": "esno scripts/postbuild.ts",
    "dev": "export LOG_DEBUG=1 && tsup --watch src",
    "play": "npm -C playground/browser run dev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SepVeneto/bury-report"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "./src/browser/polyfill.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./mp": {
      "types": "./dist/mp-uni/index.d.ts",
      "require": "./dist/mp-uni/index.cjs",
      "import": "./dist/mp-uni/index.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rspack": {
      "types": "./dist/rspack.d.ts",
      "require": "./dist/rspack.cjs",
      "import": "./dist/rspack.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "dependencies": {
    "unplugin": "^1.5.1"
  },
  "devDependencies": {
    "@babel/parser": "^7.23.6",
    "@dcloudio/types": "^3.4.3",
    "@rrweb/record": "2.0.0-alpha.18",
    "@rrweb/types": "2.0.0-alpha.18",
    "@sepveneto/eslint-config": "^0.9.1",
    "@swc/core": "^1.7.3",
    "@types/node": "^20.10.5",
    "@types/pako": "^2.0.4",
    "chalk": "^5.3.0",
    "core-js": "^3.39.0",
    "debug": "^3",
    "esbuild": "^0.25.3",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "fast-glob": "^3.3.2",
    "html-webpack-plugin": "^5.6.3",
    "local-pkg": "^0.5.0",
    "magic-string": "^0.30.5",
    "miniprogram-api-typings": "^3.12.2",
    "nodemon": "^3.0.2",
    "rollup": "^4.9.1",
    "safe-area-insets": "^1.4.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "webpack": "^5.89.0"
  }
}
